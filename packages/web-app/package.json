{
  "name": "@supranorma/web-app",
  "version": "0.1.0",
  "description": "Full-stack web application for project management and collaboration",
  "private": true,
  "scripts": {
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc -p tsconfig.backend.json",
    "build:frontend": "cd frontend && npm run build",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "tsx watch backend/src/server.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint backend/**/*.ts frontend/src/**/*.{ts,tsx}"
  },
  "keywords": ["web", "app", "full-stack", "react", "express"],
  "author": "Hyperpolymath",
  "license": "MIT",
  "dependencies": {
    "@supranorma/shared": "^0.1.0",
    "@supranorma/ai-core": "^0.1.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "zod": "^3.22.4",
    "better-sqlite3": "^9.2.2",
    "drizzle-orm": "^0.29.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.8",
    "tsx": "^4.7.0",
    "drizzle-kit": "^0.20.9",
    "concurrently": "^8.2.2",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  }
}
